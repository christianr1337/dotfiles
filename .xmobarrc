Config  { font = "-xos4-terminus-medium-r-normal--14-140-72-72-c-80-iso8859-1"
        , bgColor = "black"
        , fgColor = "grey"
        , position = BottomW L 100
        , commands =  [ Run Cpu ["-t", "CPU: <total>%", "-L", "3", "-H", "50", "--normal", "green", "--high", "red"] 10 
                      , Run Memory ["-t", "Mem: <usedratio>%"] 10
                      , Run Battery ["-t", "Batt: <left>%"] 10
                      , Run Date "%a %b %_d %k:%M" "date" 10
                      , Run Weather "EKBI" ["-t", "<skyCondition> <tempC>C", "-L", "10", "-H", "30", "--normal", "green", "--high", "red"] 100
                      , Run StdinReader
                      ]
         , sepChar = "%"
         , alignSep = "}{"
         , template = "%StdinReader% }{ %cpu% | %memory% | %battery% - <fc=#ee9a00>%date%</fc> - %EKBI%"
        }

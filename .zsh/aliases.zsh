# Directory tree navigation
alias -g ..='cd ..'
alias -g ...='cd ../..'

# Files by suffix
alias -s c=$EDITOR
alias -s com=$BROWSER
alias -s h=$EDITOR
alias -s html=$BROWSER
alias -s js=$EDITOR
alias -s org=$BROWSER
alias -s py=$EDITOR

# Useful
alias -g LC='| wc -l'
alias -g DN='/dev/null'
alias -g L='| less'
alias -g NUL='> /dev/null 2>&1'

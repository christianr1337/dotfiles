	email = alejandroogomez@gmail.com
[core]
	editor = vim
[alias]
    a = add
    ap = add -p
    b = branch -va
    c = commit
    cm = commit -m
    co = checkout 
    d = diff --word-diff
    l = log --graph --decorate --no-merges --oneline
    lp = log --graph --decorate --pretty=short -p --word-diff
    m = merge
    ph = push
    pl = pull --rebase
    s = status -sb
    st = stash
    sta = stash apply
    stp = stash pop
    discard = reset HEAD --hard
[color]
	ui = true
[merge]
    tool = vimdiff
[push]
    default = tracking
[branch]
    autosetuprebase = always
